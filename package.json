{
  "name": "ynlu",
  "description": "Yoctol Natural Language Understanding SDK for nodejs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoctol/yoctol-nlu-node.git"
  },
  "version": "0.5.2",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "test": "npm run lint:fix && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand",
    "testonly:watch": "jest --watch",
    "preversion": "npm test"
  },
  "dependencies": {
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "invariant": "^2.2.4",
    "node-fetch": "^2.3.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-yoctol-base": "^0.19.1",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "prettier-package-json": "^2.1.0"
  },
  "keywords": [
    "language",
    "nlp",
    "nlu",
    "understanding",
    "yoctol"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
